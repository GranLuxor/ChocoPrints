        <item name="colorPrimary">@color/colorEncabezado</item>
        <item name="colorPrimaryDark">@color/colorBarraSuperior</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="botones">
        <item name="android:layout_width">@dimen/largo</item>
        <item name="android:layout_height">@dimen/largo</item>
